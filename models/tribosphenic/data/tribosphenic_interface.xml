<?xml version="1.0" encoding="UTF-8" ?>

<Interface>

<General>
    <Name>Tribosphenic tooth</Name>
    <DefaultParameters>BastP2.txt</DefaultParameters>
</General>
    
<Binary>
    <BinaryOSX>humppa_translate_mac</BinaryOSX>
    <BinaryLinux>humppa_translate_linux</BinaryLinux>
    <BinaryWindows>humppa_translate.exe</BinaryWindows>
    <InputStyle>Humppa</InputStyle>
    <OutputStyle>Humppa</OutputStyle>
    <OutputParser>dad_to_polygons</OutputParser>
    <ResultParser>top_cusp_angle</ResultParser>
</Binary>

<Controls>
    <ModelStepsize>500</ModelStepsize>
    <ParametersImage>bg_text_quadrate.png</ParametersImage>
    <CellConnections>Enable</CellConnections>
    <Orientation>
        <Name>Occlusal</Name>
        <Rotate>0.0,0.0</Rotate>
    </Orientation>
    <Orientation>
        <Name>Buccal</Name>
        <Rotate>180.0,-90.0</Rotate>
    </Orientation>
    <Orientation>
        <Name>Lingual</Name>
        <Rotate>0.0,-90.0</Rotate>
    </Orientation>
    <Orientation>
        <Name>Posterior</Name>
        <Rotate>90.0,-90.0</Rotate>
    </Orientation>
    <Orientation>
        <Name>Anterior</Name>
        <Rotate>-90.0,-90.0</Rotate>
    </Orientation>
    
    <ViewMode>
        <Name>Shape only</Name>
        <Content>0</Content>
    </ViewMode>
    <ViewMode>
        <Name>Differentiation &amp; knots</Name>
        <Content>1</Content>
    </ViewMode>
    <ViewMode>
        <Name>Activator concentration</Name>
        <Content>2</Content>
    </ViewMode>
    <ViewMode>
        <Name>Inhibitor concentration</Name>
        <Content>3</Content>
    </ViewMode>
    <ViewMode>
        <Name>Growth factor concentration</Name>
        <Content>4</Content>
    </ViewMode>    
</Controls>

<Parameters>
    <Parameter>
        <Name>Egr</Name>
        <Button>260,35</Button>
        <ButtonWidth>67</ButtonWidth>
        <Description>Epithelial proliferation rate.</Description>
        <Hidden>False</Hidden>
    </Parameter>
    <Parameter>
        <Name>Mgr</Name>
        <Button>230,325</Button>
        <ButtonWidth>67</ButtonWidth>
        <Description>Mesenchymal proliferation rate.</Description>
        <Hidden>False</Hidden>
    </Parameter>
    <Parameter>
        <Name>Rep</Name>
        <Button>260,170</Button>
        <ButtonWidth>67</ButtonWidth>
        <Description>Young's modulus (stiffness).</Description>
        <Hidden>False</Hidden>
    </Parameter>
    <Parameter>
        <Name>Swi</Name>
        <Button>185,457</Button>
        <ButtonWidth>67</ButtonWidth>
        <Description>Distance from 0 where the borders are defined.</Description>
        <Hidden>False</Hidden>
    </Parameter>    
    <Parameter>
        <Name>Adh</Name>
        <Button>260,145</Button>
        <ButtonWidth>67</ButtonWidth>
