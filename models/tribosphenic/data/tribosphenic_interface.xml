        <Hidden>False</Hidden>
    </Parameter>    
    <Parameter>
        <Name>Inh</Name>
        <Button>33,430</Button>
        <ButtonWidth>67</ButtonWidth>
        <Description>Inhibition of activator.</Description>
        <Hidden>False</Hidden>
    </Parameter>
    <Parameter>
        <Name>Not2</Name>
        <Value>0.0</Value>
        <Button>33,380</Button>
        <ButtonWidth>67</ButtonWidth>
