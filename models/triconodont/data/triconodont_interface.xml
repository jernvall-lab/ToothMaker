<?xml version="1.0" encoding="UTF-8" ?>

<Interface>

<General>
    <Name>Triconodont tooth</Name>
    <DefaultParameters>seal_triconodont.txt</DefaultParameters>
</General>
    
<Binary>
    <BinaryOSX>humppa_mac</BinaryOSX>
    <BinaryLinux>humppa_linux</BinaryLinux>
    <BinaryWindows>humppa.exe</BinaryWindows>
    <InputStyle>Humppa</InputStyle>
    <OutputStyle>Humppa</OutputStyle>
    <OutputParser>no_empty_lines</OutputParser>
    <OutputParser>dad_to_polygons</OutputParser>
    <ResultParser>top_cusp_angle</ResultParser>
</Binary>
    
<Controls>
    <ModelStepsize>500</ModelStepsize>
    <ParametersImage>bg_text_export.png</ParametersImage>
    <CellConnections>Enable</CellConnections>
    <Orientation>
        <Name>Occlusal</Name>
        <Rotate>0.0,0.0</Rotate>
    </Orientation>
    <Orientation>
        <Name>Buccal</Name>
        <Rotate>0.0,-90.0</Rotate>
    </Orientation>
    <Orientation>
        <Name>Lingual</Name>
        <Rotate>180.0,-90.0</Rotate>
    </Orientation>
    <Orientation>
        <Name>Posterior</Name>
        <Rotate>90.0,-90.0</Rotate>
    </Orientation>
    <Orientation>
        <Name>Anterior</Name>
        <Rotate>-90.0,-90.0</Rotate>
    </Orientation>

    <ViewMode>
        <Name>Shape only</Name>
        <Content>0</Content>
    </ViewMode>
    <ViewMode>
        <Name>Differentiation &amp; knots</Name>
        <Content>1</Content>
    </ViewMode>
    <ViewMode>
        <Name>Activator concentration</Name>
        <Content>2</Content>
    </ViewMode>
    <ViewMode>
        <Name>Inhibitor concentration</Name>
        <Content>3</Content>
    </ViewMode>
    <ViewMode>
        <Name>FGF concentration</Name>
        <Content>4</Content>
    </ViewMode>    
</Controls>

<Parameters>
    <Parameter>
        <Name>Egr</Name>
        <Button>260,35</Button>
        <ButtonWidth>67</ButtonWidth>
        <Description>Epithelial proliferation rate.</Description>
        <Hidden>False</Hidden>
    </Parameter>
    <Parameter>
        <Name>Mgr</Name>
        <Button>230,325</Button>
        <ButtonWidth>67</ButtonWidth>
        <Description>Mesenchymal proliferation rate.</Description>
        <Hidden>False</Hidden>
    </Parameter>
    <Parameter>
        <Name>Rep</Name>
        <Button>260,170</Button>
        <ButtonWidth>67</ButtonWidth>
        <Description>Young's modulus (stiffness).</Description>
        <Hidden>False</Hidden>
    </Parameter>
    <Parameter>
        <Name>Not</Name>
        <Value>0.0</Value>
        <Button>280,115</Button>
        <ButtonWidth>67</ButtonWidth>
        <Description>Nothing (in mouse it is the distance from 0 where the borders are defined).</Description>
        <Hidden>True</Hidden>
    </Parameter>    
    <Parameter>
        <Name>Adh</Name>
        <Button>260,145</Button>
        <ButtonWidth>67</ButtonWidth>
        <Description>Traction between neighbours.</Description>
        <Hidden>False</Hidden>
    </Parameter>
    
    <Parameter>
        <Name>Act</Name>
        <Button>33,245</Button>
        <ButtonWidth>67</ButtonWidth>
        <Description>BMP4 auto-activation.</Description>
        <Hidden>False</Hidden>
    </Parameter>    
    <Parameter>
        <Name>Inh</Name>
        <Button>33,380</Button>
        <ButtonWidth>67</ButtonWidth>
        <Description>Inhibition of SHH over BMP4.</Description>
        <Hidden>False</Hidden>
    </Parameter>
    <Parameter>
        <Name>Not2</Name>
        <Value>0.0</Value>
        <Button>33,380</Button>
        <ButtonWidth>67</ButtonWidth>
        <Description>Nothing (ectodin in mouse).</Description>
        <Hidden>True</Hidden>
    </Parameter>    
    <Parameter>
        <Name>Sec</Name>
        <Button>33,135</Button>
        <ButtonWidth>67</ButtonWidth>
        <Description>FGF4 secretion rate.</Description>
        <Hidden>False</Hidden>
    </Parameter>
    <Parameter>
        <Name>Not3</Name>
        <Value>0.0</Value>
        <Button>33,135</Button>
        <ButtonWidth>67</ButtonWidth>
        <Description>Nothing.</Description>
        <Hidden>True</Hidden>
    </Parameter>
    
    <Parameter>
        <Name>Da</Name>
        <Button>33,270</Button>
        <ButtonWidth>67</ButtonWidth>
        <Description>BMP4 diffusion rate.</Description>
        <Hidden>False</Hidden>
    </Parameter>
    <Parameter>
        <Name>Di</Name>
        <Button>33,405</Button>
        <ButtonWidth>67</ButtonWidth>
        <Description>SHH diffusion rate.</Description>
        <Hidden>False</Hidden>
    </Parameter>    
    <Parameter>
        <Name>Ds</Name>
        <Button>33,160</Button>
        <ButtonWidth>67</ButtonWidth>
        <Description>FGF4 diffusion rate.</Description>
        <Hidden>False</Hidden>
    </Parameter>
    <Parameter>
        <Name>Not4</Name>
    	<Value>0.0</Value>
        <Button>33,160</Button>
        <ButtonWidth>67</ButtonWidth>
        <Description>Nothing (ectodin diffusion).</Description>
        <Hidden>True</Hidden>
    </Parameter>    
    <Parameter>
        <Name>Int</Name>
        <Button>33,355</Button>
        <ButtonWidth>67</ButtonWidth>
        <Description>Initial (SHH) threshold.</Description>
        <Hidden>False</Hidden>
    </Parameter>
    
    <Parameter>
        <Name>Set</Name>
        <Button>33,110</Button>
        <ButtonWidth>67</ButtonWidth>
        <Description>Secondary (FGF4) threshold.</Description>
        <Hidden>False</Hidden>
    </Parameter>    
    <Parameter>
        <Name>Boy</Name>
        <Button>230,375</Button>
        <ButtonWidth>67</ButtonWidth>
        <Description>Mesenchyme mechanic resistance.</Description>
        <Hidden>False</Hidden>
    </Parameter>
    <Parameter>
        <Name>Dff</Name>
        <Button>260,105</Button>
        <ButtonWidth>67</ButtonWidth>
        <Description>Differentiation rate.</Description>
        <Hidden>False</Hidden>
    </Parameter>    
    <Parameter>
        <Name>Bgr</Name>
        <Button>230,350</Button>
        <ButtonWidth>67</ButtonWidth>
        <Description>Border growth, amount of mesenchyme in anterior-posterior.</Description>
        <Hidden>False</Hidden>
    </Parameter>
    <Parameter>
        <Name>Abi</Name>
        <Button>260,235</Button>
        <ButtonWidth>67</ButtonWidth>
        <Description>Anterior bias.</Description>
        <Hidden>False</Hidden>
    </Parameter>
    
    <Parameter>
        <Name>Pbi</Name>
        <Button>260,260</Button>
        <ButtonWidth>67</ButtonWidth>
        <Description>Posterior bias.</Description>
        <Hidden>False</Hidden>
    </Parameter>
    <Parameter>
        <Name>Bbi</Name>
        <Value>1.0</Value>
        <Button>300,185</Button>
        <ButtonWidth>67</ButtonWidth>
        <Description>Buccal bias.</Description>
        <Hidden>True</Hidden>
    </Parameter>    
    <Parameter>
        <Name>Lbi</Name>
        <Value>1.0</Value>
        <Button>300,260</Button>
        <ButtonWidth>67</ButtonWidth>
        <Description>Lingual bias.</Description>
        <Hidden>True</Hidden>
    </Parameter>
    <Parameter>
        <Name>Rad</Name>
        <Button>270,432</Button>
        <ButtonWidth>67</ButtonWidth>
