        <Hidden>False</Hidden>
    </Parameter>    
    <Parameter>
        <Name>Deg</Name>
        <Button>33,30</Button>
        <ButtonWidth>67</ButtonWidth>
        <Description>Protein degradation rate.</Description>
        <Hidden>False</Hidden>
    </Parameter>
    
    <Parameter>
        <Name>Dgr</Name>
        <Button>260,80</Button>
        <ButtonWidth>67</ButtonWidth>
        <Description>Downward vector of growth.</Description>
        <Hidden>False</Hidden>
    </Parameter>    
    <Parameter>
        <Name>Ntr</Name>
        <Button>260,195</Button>
        <ButtonWidth>67</ButtonWidth>
        <Description>Mechanical traction from the borders to the nucleus.</Description>
        <Hidden>False</Hidden>
    </Parameter>
    <Parameter>
        <Name>Bwi</Name>
        <Value>0.8</Value>
        <Button>305,457</Button>
        <ButtonWidth>67</ButtonWidth>
        <Description>Width of border.</Description>
        <Hidden>True</Hidden>
    </Parameter>       
</Parameters>

</Interface>

